#include<bits/stdc++.h>
using namespace std;

int main() {
    int arr[6] = {1,2,1,1,3,2};
    int n = 6;
    int i = 0;
    int j = 1;
    sort(arr , arr+n);  // [1 , 1 , 1 , 2 , 2 , 3 ]
    for (j ; j < n ; j++){
        if (arr[i] != arr[j]) 
        {
            // i++;
            arr[i+1]  = arr[j];
                i++;  
            // cout << i << " " << j << endl;

        }

    }
  
    cout << i+1;
    
}
